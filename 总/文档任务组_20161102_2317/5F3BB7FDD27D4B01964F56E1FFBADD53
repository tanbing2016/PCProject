<script type="text/javascript">
	//复制路径
	function CopyAll(T) {
		var id = document.getElementById("wanto");
		var rng;
		var bookmark;
		if (document.createRange) {
			rng = document.createRange();
			rng.selectNodeContents(id);
			var sel = document.getSelection();
			sel.removeAllRanges();
			sel.addRange(rng);
		} else {
			rng = document.body.createTextRange();
			bookmark = rng.getBookmark();
			rng.moveToElementText(id);
			rng.scrollIntoView();
			rng.select();
			rng.execCommand("Copy");
			rng.collapse(false);
		}
		alert("复制成功!");
	}
</script>
